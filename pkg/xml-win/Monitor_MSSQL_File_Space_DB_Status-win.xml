			<long_description>Free space in MB avaiable in DB</long_description>
			<validation_rule/>
			<error_message/>
			<gui_type><decimal/></gui_type>
		</element>
		<element name='DBFreeSpacePCT' parameter_type='output' data_type='ranged' gui_basic='' range_type='0' units='%'>
			<control_options>size:8</control_options>
			<default_value></default_value>
			<short_description>DB Free Space PCT</short_description>
			<long_description>Free space percentage available in DB</long_description>
			<validation_rule/>
			<error_message/>
			<gui_type><decimal/></gui_type>
		</element>
		<element name='LogFileFreeSpaceMB' parameter_type='output' data_type='ranged' gui_basic='' range_type='0' units=''>
			<control_options>size:8</control_options>
			<default_value></default_value>
			<short_description>Log File Free Space MB</short_description>
			<long_description>Log file free space in MB available for the DB</long_description>
			<validation_rule/>
			<error_message/>
			<gui_type><decimal/></gui_type>
		</element>
		<element name='LogFileFreeSpacePCT' parameter_type='output' data_type='ranged' gui_basic='' range_type='0' units='%'>
			<control_options>size:8</control_options>
			<default_value></default_value>
			<short_description>Log File Free Space PCT</short_description>
			<long_description>Log file free space percentage available for the DB</long_description>
			<validation_rule/>
			<error_message/>
			<gui_type><decimal/></gui_type>
		</element>
		<element name="timer" parameter_type="output" data_type="integer" gui_basic="0" range_type="0" units="ms">
			<control_options>size:8</control_options>
			<default_value/>
			<short_description>Response time</short_description>
			<long_description>Response time in ms</long_description>
			<validation_rule/>
			<error_message/>
			<gui_type><integer/></gui_type>
		</element>
	</elements>
	<help>When running a custom script check, note that the second parameter is always the name of the assigned host. The script that's executed should output each variable on it's own line, otherwise up.time won't be able to capture the appropriate information.</help>
</uptime>